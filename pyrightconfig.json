{
    "venvPath": ".",
    "venv": ".venv",
    "pythonVersion": "3.12",
    "typeCheckingMode": "basic",
    "reportMissingImports": "warning",
    "reportMissingTypeStubs": false,
    "include": ["src"],
    "exclude": [
        "**/__pycache__",
        ".venv",
        "qdrant_db",
        "storage"
    ]
}


